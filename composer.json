{
    "name": "cultuurnet/iis-udb3-store",
    "description": "A PHP library to interact with IIS-UDB3 CdbXml store.",
    "type": "library",
    "license": "Apache-2.0",
    "authors": [],
    "require": {
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5",
        "doctrine/migrations": "^1.4",
        "nicolopignatelli/valueobjects": "~2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "~2.3",
        "phing/phing": "~2.11",
        "satooshi/php-coveralls": "~0.7"
    },
    "autoload": {
        "psr-4": {
            "CultuurNet\\UDB3\\IISStore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CultuurNet\\UDB3\\IISStore\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
